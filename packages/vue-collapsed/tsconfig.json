{
   "compilerOptions": {
      "target": "ES2015",
      "module": "ES2015",
      "moduleResolution": "Node",
      "strict": true,
      "isolatedModules": true,
      "lib": ["ESNext", "ES2015", "DOM"],
      "skipLibCheck": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
   }
}
